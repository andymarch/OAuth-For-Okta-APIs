<body>
    <script>new ClipboardJS('.copyButton');</script>
    <script>
        function toggle(element) {
            var x = document.getElementById(element);
            if (x.style.display === "none") {
                x.style.display = "flex";
            } else {
                x.style.display = "none";
            }
        }
    </script>
